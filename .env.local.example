# Weather API Key (e.g., OpenWeatherMap)
# Get yours from: https://openweathermap.org/api
NEXT_PUBLIC_WEATHER_API_KEY=YOUR_API_KEY_HERE

# Set to true to use mock weather data instead of calling the API
# Useful for development or if you don't have an API key yet.
# Remove this line or set to false to use the real API (requires NEXT_PUBLIC_WEATHER_API_KEY).
NEXT_PUBLIC_MOCK_WEATHER_API=true

# Optional: Geocoding API Key (if implementing location string to coordinates)
# NEXT_PUBLIC_GEOCODING_API_KEY=YOUR_GEOCODING_KEY_HERE
